{
  "name": "lottie-minify-plugin",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "default": "./dist/vite.js",
      "require": "./dist/vite.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/index.d.ts"],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "pub": "npm publish"
  },
  "keywords": [],
  "author": "yanyuhao.joy",
  "license": "ISC",
  "dependencies": {
    "lottie-type": "workspace:*",
    "@rollup/pluginutils": "^4.1.2",
    "lottie-minify": "workspace:*",
    "unplugin": "^0.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  }
}
